{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "node",
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "moduleResolution": "node",
    "paths": {
      "@badeball/cypress-cucumber-preprocessor/*": [
        "./node_modules/@badeball/cypress-cucumber-preprocessor/dist/subpath-entrypoints/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ]
}